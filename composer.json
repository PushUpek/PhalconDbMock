{
  "name": "nicklewis/phalcon-db-mock",
  "type": "library",
  "description": "Db Mock adds a mock database that you can insert into, delete, and select from for phpunit mocks",
  "keywords": [
    "phpunit",
    "phalcon"
  ],
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Nicholas Lewis",
      "email": "nickle799@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ext-phalcon": "3.*",
    "greenlion/php-sql-parser": "4.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*",
    "phpunit/php-code-coverage": "4.0.*"
  },
  "autoload": {
    "psr-0": {
      "NickLewis\\PhalconDbMock": "Library/"
    }
  }
}
